  background-color: transparent;
}
.section-bloglist .article-card .card__heading {
  font-family: 'Flexo Bold';
  font-size: 22px;
  letter-spacing: 0px;
  transition: 0.3s;
}
.section-bloglist .article.grid__item:hover .card__heading {
  color: #387201;
}
.section-bloglist .article-card__excerpt {
  color: #151515;
  line-height: 1.6;
}


